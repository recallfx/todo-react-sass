{
  "name": "todo-react-sass",
  "version": "0.0.1",
  "description": "Example todo list application based on bootstrap sass, react and webpack",
  "engines": {
    "node": "5"
  },
  "scripts": {
    "start": "npm run dev",

    "dev": "nodemon --watch server.dev.js server.dev.js",
    "lint": "eslint --ext .js,.jsx .",
    "build": "set WEBPACK_ENV=production && webpack --config webpack.prod.config.js",
    "prod": "set NODE_ENV=production && npm run build && nodemon --watch server.prod.js server.prod.js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/recallfx/todo-react-sass.git"
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "bootstrap": "^3.3.5",
    "bootstrap-sass": "^3.3.5",
    "express": "^4.13.4",
    "font-awesome": "^4.5.0",
    "jquery": "^2.2.4",
    "lodash": "^4.13.1",
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "tether": "^1.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.1.0",
    "babel": "^6.3.13",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-eslint": "^6.0.5",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "bootstrap-loader": "^1.0.9",
    "css-loader": "^0.23.0",
    "eslint": "^1.9.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^3.8.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.4",
    "flux": "^2.1.1",
    "font-awesome-loader": "^0.0.1",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.1",
    "postcss-loader": "^0.8.1",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.1.1",
    "resolve-url-loader": "^1.4.3",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.6",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.5.0"
  }
}
